#parse('textSearch.vm')
<div class="modal fade" id="textSearchModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-textSearch-title">#springMessage('modal.title.concept.search')</h4>
      </div>
      <div class="modal-body">
        <div id="textSearchTemplate">
          <script type="text/x-handlebars" data-template-name="textsearch-output">
            {{control "textSearch"}}
        </script>
        </div>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->