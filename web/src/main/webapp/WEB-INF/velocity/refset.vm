#set ($title = "#springMessageText('page.title.refset' [$refset.title])")
<div id="refset">
 <div class="return">
    <a href='#springUrl("/refsets")'>Return</a>
  </div>
  <table>
    <tr>
        <td class="heading">Title:</td>
        <td>$refset.title</td>
    </tr>
    <tr>
        <td class="description">Description:</td>
        <td>$refset.description</td>
    </tr>
   <tr>
        <td class="publicid">Public Id:</td>
        <td>$refset.publicId</td>
    </tr>
  </table>
  <div class="edit">
    <a href='#springUrl("/refset/$refset.publicId/edit")'><input type="button" value="edit"/></a>
  </div>
</div>